function countVowels(str) {
str = str.toLowerCase();
    let count = 0;
for (let i = 0; i < str.length; i++) {
     if ('aeiou'.includes(str[i])) {
            count++;
        }
    }

    return count;
}
console.log(countVowels("Hello World")); 
